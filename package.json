{
	"name": "serverless-boilerplate-express-typescript",
	"version": "1.0.0",
	"description": "Serverless framework ExpressJS TypeScript",
	"scripts": {
		"dev:db": "docker-compose up -d",
		"dev:server": "NODE_ENV=development sls offline start --stage development",
		"dev": "run-p dev:*",
		"tail-log": "sls logs -f app -t",
		"deploy-prod": "NODE_ENV=production sls deploy --stage production",
		"remove-prod": "NODE_ENV=production sls remove --stage production",
		"clean": "rimraf dist .serverless",
		"lint": "eslint --ext .js,.ts .",
		"check-types": "tsc --noEmit --pretty",
		"test": "jest",
		"postinstall": "prisma generate"
	},
	"dependencies": {
		"@prisma/client": "^4.6.1",
		"express": "^4.18.2",
		"serverless-http": "^3.1.0"
	},
	"devDependencies": {
		"@swc/core": "^1.3.19",
		"@swc/jest": "^0.2.23",
		"@types/express": "^4.17.14",
		"@types/jest": "^29.2.3",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.44.0",
		"@typescript-eslint/parser": "^5.44.0",
		"esbuild": "^0.16.7",
		"eslint": "^8.28.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^27.1.5",
		"eslint-plugin-jest-formatting": "^3.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-simple-import-sort": "^8.0.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"jest": "^29.3.1",
		"lint-staged": "^13.0.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.0",
		"prisma": "^4.6.1",
		"serverless": "^3.25.0",
		"serverless-dotenv-plugin": "^4.0.2",
		"serverless-esbuild": "^1.34.0",
		"serverless-offline": "^11.5.0",
		"supertest": "^6.3.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.3"
	},
	"author": "Vox"
}
